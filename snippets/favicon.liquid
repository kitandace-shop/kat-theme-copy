<link rel="apple-touch-icon" sizes="180x180" href="{{ 'apple-touch-icon.png' | file_img_url }}">
{% if settings.favicon != blank %}
    <link rel="shortcut icon" href="{{ settings.favicon | image_url: width: 32, height: 32 }}" type="image/png">
{% endif %}
<link rel="icon" type="image/png" sizes="16x16" href="{{ 'favicon-16x16.png' | file_img_url }}">
<link rel="manifest" href="{{ 'site.webmanifest' | file_url }}">
<link rel="mask-icon" href="{{ 'safari-pinned-tab.svg' | file_url }}" color="#003096">
<meta name="msapplication-TileColor" content="#2d89ef">
<meta name="msapplication-config" content="{{ 'browserconfig.xml' | file_url }}">
<meta name="theme-color" content="#003096">
